{
  "name": "pricecheck",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev --host",
    "build": "npm run check && npm run lint && svelte-kit build",
    "start": "npm run build && svelte-kit preview --port=3333",
    "prettier": "prettier --ignore-path .gitignore --plugin-search-dir=. .",
    "check": "npm run prettier -- --check",
    "format": "npm run prettier -- --write",
    "lint": "eslint --ignore-path .gitignore .",
    "husky": "husky install && husky add .husky/pre-commit \"npm run check && npm run lint\"",
    "prisma-db-push": "prisma db push",
    "prisma-generate": "prisma generate",
    "prisma": "prisma generate && prisma db push",
    "prisma-studio": "prisma studio",
    "prisma-format": "prisma format"
  },
  "devDependencies": {
    "@prisma/client": "3.14.0",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@tailwindcss/forms": "0.5.2",
    "autoprefixer": "10.4.7",
    "eslint": "8.16.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-svelte3": "4.0.0",
    "husky": "8.0.1",
    "postcss": "8.4.14",
    "postcss-load-config": "3.1.4",
    "prettier": "2.6.2",
    "prettier-plugin-svelte": "2.7.0",
    "prettier-plugin-tailwindcss": "0.1.11",
    "svelte": "3.48.0",
    "svelte-preprocess": "4.10.6",
    "tailwindcss": "3.0.24"
  },
  "dependencies": {
    "prisma": "3.14.0"
  },
  "type": "module"
}
